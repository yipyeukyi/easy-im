{
  "name": "whereof/easy-im",
  "license": "MIT",
  "description": "非官方即时通信 IM，支持腾讯IM，环信IM，极光IM，融云IM，网易云信IM等",
  "homepage": "https://github.com/whereof/easy-im",
  "keywords": [
    "easy-im",
    "IM",
    "腾讯IM",
    "环信IM",
    "极光IM",
    "融云IM",
    "网易云信IM"
  ],
  "authors": [
    {
      "name": "whereof",
      "email": "wangzhiqiang1224@gmail.com"
    }
  ],
  "require": {
    "php": ">7.0",
    "ext-zlib": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^6|^7",
    "pimple/pimple": "^3.0",
    "whereof/cache": "^1.0",
    "whereof/logger": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "whereof\\easyIm\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6|^7|^8|^9",
    "mockery/mockery": "^1.2.3"
  },
  "autoload-dev": {
    "psr-4": {
      "whereof\\easyIm\\Tests\\": "tests/"
    }
  }
}
